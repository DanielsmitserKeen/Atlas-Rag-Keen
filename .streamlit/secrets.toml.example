# Streamlit Cloud Secrets Configuration
# Copy this file to secrets.toml for local development
# For Streamlit Cloud deployment: Add these variables in the Secrets section

# OpenAI API Key
OPENAI_API_KEY = "your_openai_api_key_here"

# Supabase Database Connection
# Format: postgresql://postgres:[YOUR-PASSWORD]@[HOST]:5432/postgres
SUPABASE_DB_URL = "postgresql://postgres:your_password@db.yourproject.supabase.co:5432/postgres"
